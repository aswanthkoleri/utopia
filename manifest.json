{
  "name": "utopia",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Utopia is an amazing chrome extension to override existing new tab.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Utopia",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "http://*/",
    "notifications",
    "geolocation",
    "fileSystem"
  ]
}